# Copy this file to secrets.yml and fill in your actual values
# NEVER commit secrets.yml to version control!

---
# Server Configuration
SERVER_PORT: "8080"

# Role-Based Model Configuration
# PROPOSER Configuration
PROPOSER_API_KEY: "sk-proj-your-openai-api-key-here"
PROPOSER_MODEL: "gpt-4-turbo-preview"
PROPOSER_BASE_URL: ""  # Leave empty for default, set for local models

# CHALLENGER Configuration
CHALLENGER_API_KEY: "your-gemini-api-key-here"
CHALLENGER_MODEL: "gemini-2.0-flash-exp"
CHALLENGER_BASE_URL: ""  # Leave empty for default, set for local models

# Security Configuration (set SECURITY_ENABLED to "false" to disable)
SECURITY_ENABLED: "true"
SECURITY_USERNAME: "admin"
SECURITY_PASSWORD: "your-password-here"
